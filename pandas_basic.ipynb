      "metadata": {},
      "outputs": [],
      "source": [
        "df = pd.DataFrame({\n",
        "    'A': [1, 2, 3],\n",
        "    'B': [4, 5, 6],\n",
        "    'C': ['x', 'y', 'z']\n",
        "})\n",
        "df"
      ]
    },
    {
